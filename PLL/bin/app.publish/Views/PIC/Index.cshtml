@{
    ViewBag.Title = "Account allocation setting";
}
<div>
    <div class="cls_form_pic_upload_file">
        <div class="cls_pic_data">
            <div class="panel-default">
                <div class="panel-heading cls_hand cls_div_search_criteria">
                    <h3 class="panel-title">Account allocation setting</h3>
                </div>
                <div class="panel-body cls_div_body_search_criteria" style="padding:0px;padding-top:10px;">
                    <form>
                        <div class="row">
                            <div class="col-md-2 control-label">
                                Sold To :
                            </div>

                            <div class="col-md-4">
                                <div style="display:none;" class="row tr_tfartwork_pic_sold_to_multiple tr_tfartwork_pic_sold_to_multiple_static">
                                    <div class="col-md-10">
                                        @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_pic_sold_to_multiple cls_lov_pic_sold_to_multiple_static " } })
                                    </div>
                                    <div class="col-md-1" style="padding-left: 0px;">
                                        <img class="cls_img_lov_tfartwork_pic_add_sold_to_multiple imgadd" title="Add" style="cursor:pointer;" src="~/Content/img/ico_add.png" />
                                        <img class="cls_img_lov_tfartwork_pa_delete_sold_to_multiple imgdelete" title="Delete" style="cursor:pointer;display:none;" src="~/Content/img/ico_delete.png" />
                                    </div>
                                </div>
                            </div>

                        </div>
                        <br />
                        <div class="row">
                            <div class="col-md-2 control-label">
                                Ship To :
                            </div>
                            <div class="col-md-4">
                                <div style="display:none;" class="row tr_tfartwork_pic_ship_to_multiple tr_tfartwork_pic_ship_to_multiple_static">
                                    <div class="col-md-10">
                                        @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_pic_ship_to_multiple cls_lov_pic_ship_to_multiple_static " } })
                                    </div>
                                    <div class="col-md-1" style="padding-left: 0px;">
                                        <img class="cls_img_lov_tfartwork_pic_add_ship_to_multiple imgadd" title="Add" style="cursor:pointer;" src="~/Content/img/ico_add.png" />
                                        <img class="cls_img_lov_tfartwork_pa_delete_ship_to_multiple imgdelete" title="Delete" style="cursor:pointer;display:none;" src="~/Content/img/ico_delete.png" />
                                    </div>
                                </div>
                            </div>

                        </div>
                        <br />
                        <div class="row">
                            <div class="col-md-2 control-label">
                                Zone :
                            </div>
                            <div class="col-md-4">
                                <div style="display:none;" class="row tr_tfartwork_pic_zone_multiple tr_tfartwork_pic_zone_multiple_static">
                                    <div class="col-md-10">
                                        @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_pic_zone_multiple cls_lov_pic_zone_multiple_static " } })
                                    </div>
                                    <div class="col-md-1" style="padding-left: 0px;">
                                        <img class="cls_img_lov_tfartwork_pic_add_zone_multiple imgadd" title="Add" style="cursor:pointer;" src="~/Content/img/ico_add.png" />
                                        <img class="cls_img_lov_tfartwork_pa_delete_zone_multiple imgdelete" title="Delete" style="cursor:pointer;display:none;" src="~/Content/img/ico_delete.png" />
                                    </div>
                                </div>
                            </div>

                        </div>
                        <br />
                        <div class="row">
                            <div class="col-md-2 control-label">
                                Country :
                            </div>
                            <div class="col-md-4">
                                <div style="display:none;" class="row tr_tfartwork_pic_country_multiple tr_tfartwork_pic_country_multiple_static">
                                    <div class="col-md-10">
                                        @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_pic_country_multiple cls_lov_pic_country_multiple_static " } })
                                    </div>
                                    <div class="col-md-1" style="padding-left: 0px;">
                                        <img class="cls_img_lov_tfartwork_pic_add_country_multiple imgadd" title="Add" style="cursor:pointer;" src="~/Content/img/ico_add.png" />
                                        <img class="cls_img_lov_tfartwork_pa_delete_country_multiple imgdelete" title="Delete" style="cursor:pointer;display:none;" src="~/Content/img/ico_delete.png" />
                                    </div>
                                </div>
                            </div>

                        </div>
                        <br />
                        <div class="row">
                            <div class="col-md-2 control-label">
                                Person in charge (PIC) :
                            </div>
                            <div class="col-md-4">
                                <div style="display:none;" class="row tr_tfartwork_pic_person_multiple tr_tfartwork_pic_person_multiple_static">
                                    <div class="col-md-10">
                                        @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_pic_person_multiple cls_lov_pic_person_multiple_static " } })
                                    </div>
                                    <div class="col-md-1" style="padding-left: 0px;">
                                        <img class="cls_img_lov_tfartwork_pic_add_person_multiple imgadd" title="Add" style="cursor:pointer;" src="~/Content/img/ico_add.png" />
                                        <img class="cls_img_lov_tfartwork_pa_delete_person_multiple imgdelete" title="Delete" style="cursor:pointer;display:none;" src="~/Content/img/ico_delete.png" />
                                    </div>
                                </div>
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <button type="submit" class="cls_btn_search btn btn-primary">Search</button>
                                <button type="button" class="cls_btn_pic_clear btn btn-danger">Clear</button>
                                <button type="button" class="cls_btn_pic_export btn btn-info">Export Excel</button>
                                <button type="button" class="cls_btn_pic_upload_add btn btn-warning">Upload Excel for add PIC</button>
                                <button type="button" class="cls_btn_pic_upload_edit btn btn-warning">Upload Excel for edit PIC</button>
                                <button type="button" class="cls_btn_pic_delete btn btn-danger">Delete</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <hr style="margin-top:10px;margin-bottom:10px;" />
            <div class="row">
                <div class="col-md-12 cls_table_pic_data">
                    <table id="table_pic_data" class="display" style="width:100%">
                        <thead>
                            <tr>
                                <th>
                                    <input type="checkbox" id="pic_select_all">
                                </th>
                                <th class="cls_nowrap">No. </th>
                                <th class="cls_nowrap cls_td_width_75">User name</th>
                                <th class="cls_nowrap cls_td_width_150">First name</th>
                                <th class="cls_nowrap cls_td_width_150">Last name</th>
                                <th class="cls_nowrap cls_td_width_110">Sold-to</th>
                                <th class="cls_nowrap cls_td_width_150">Sold-to name</th>
                                <th class="cls_nowrap cls_td_width_110">Ship-to</th>
                                <th class="cls_nowrap cls_td_width_150">Ship-to name</th>
                                <th class="cls_nowrap cls_td_width_75">ZONE</th>
                                <th class="cls_nowrap cls_td_width_75">Country</th>
                                <th class="cls_nowrap cls_td_width_150">Country name</th>
                                <th class="cls_nowrap cls_hide">PIC_ID</th>
                                <th class="cls_nowrap">Action</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>

        <!-- #region Modal Edit -->
        <div class="modal fade" id="modal_pic_edit" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
            <div class="vertical-alignment-helper">
                <input type="hidden" class="cls_pic_id_edit" />
                <div class="modal-dialog  modal_in_modal vertical-align-center" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h4 class="modal-title" id="myModalLabel">Edit PIC</h4>

                        </div>
                        <div class="modal-body">
                            <div id="response"></div>
                            <div class="cls_div_table_report_warehouse_so_att">
                                <div class="row">
                                    <div class="col-md-3 control-label">
                                        Sold-to<span style="color:red;">*</span> :
                                    </div>
                                    <div class="col-md-4">
                                        @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_pic_edit_sold_to" } })
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-3 control-label">
                                        Ship-to<span style="color:red;">*</span> :
                                    </div>
                                    <div class="col-md-4">
                                        @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_pic_edit_ship_to" } })
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-3 control-label">
                                        Zone<span style="color:red;">*</span> :
                                    </div>
                                    <div class="col-md-4">
                                        @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_pic_edit_zone" } })
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-3 control-label">
                                        Country :
                                    </div>
                                    <div class="col-md-4">
                                        @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_pic_edit_country" } })
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-3 control-label">
                                        Person in charge (PIC)<span style="color:red;">*</span> :
                                    </div>
                                    <div class="col-md-4">
                                        @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_pic_edit_user" } })
                                    </div>
                                </div>


                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-success cls_btn_pic_edit_save">Save</button>
                            @*<button type="button" class="btn btn-danger cls_btn_pic_edit_delete" data-dismiss="modal">Delete</button>*@
                            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- #endregion -->
        <!-- #region Modal Upload -->
        <div class="modal" id="modal_pic_upload_edit" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
            <div class="vertical-alignment-helper">

                <div class="modal-dialog  modal_in_modal vertical-align-center" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h4 class="modal-title" id="myModalLabel"></h4>
                        </div>
                        <div class="modal-body">
                            @Html.Partial("UploadPIC")
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-default cls_upload_close" data-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- #endregion -->

    </div>

</div>
@section scripts
{
    @Scripts.Render("~/bundles/picsetting")
  
}

