
@{
    ViewBag.Title = "Overall Master Data Value and Status Report";
}
<div class="cls_report_OverallMaster">
    <div class="panel-default">
        <div class="panel-heading cls_div_search_criteria">
            <h3 class="panel-title">Overall Master Data Value and Status Report</h3>
        </div>

        <div class="panel-body cls_div_body_search_criteria" style="padding:0px;padding-top:10px;">
            <form>
                <div class="row">
                    <div class="col-md-2 control-label">
                        Master :
                    </div>
                    <div class="col-md-4">
                        @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_master" } })
                    </div>
                    <div class="col-md-2 control-label ">Keyword :</div>
                    <div class="col-md-4 "><input type="text" class="form-control cls_txt_master_keyword" placeholder="" /></div>
                </div>
                <div class="row">
                    <div class="col-md-2 control-label">
                        By Status :
                    </div>
                    <div class="col-md-4">
                        <select class="form-control cls_lov_status" style="width:100%;">
                            <option value="All" selected>All</option>
                            <option value="Active">Active</option>
                            <option value="Inactive">Inactive</option>
                        </select>
                    </div>
                </div>
                <hr />
                <div class="row">
                    <div class="col-md-4">
                        <button type="submit" class="cls_overall_btn_search btn btn-primary">Search</button>
                        @*<button type="button" class="cls_overall_btn_clr btn btn-danger">Clear</button>*@
                        <button type="button" class="btn btn-info cls_btn_export_excel">Export excel</button>
                        @*<button type="button" class="btn cls_overall_btn_remove" style="background-color:darkorange;border-color:coral;color:white">Inactive</button>*@
                        @*<button type="button" class="cls_overall_btn_Remove btn btn-danger">Inactive</button>*@
                        @*<button type="button" class="cls_trk_btn_back btn btn-warning">Back</button>*@
                    </div>
                </div>


            </form>
        </div>
    </div>
    <hr style="margin-top:10px;margin-bottom:10px;" />
