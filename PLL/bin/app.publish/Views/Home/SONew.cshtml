<div class="row">
    <div style="width: 150px;" class="col-md-2">
        SO create date from :
        <input style="width:120px;" class="cls_my_datepicker form-control cls_so_new_create_date_from" />
    </div>
    <div style="width: 150px;" class="col-md-2">
        SO create date to :
        <input style="width:120px;" class="cls_my_datepicker form-control cls_so_new_create_date_to" />
    </div>
    <div style="width: 150px;" class="col-md-2">
        RDD from :
        <input style="width:120px;" class="cls_my_datepicker form-control cls_so_new_rdd_from" />
    </div>
    <div style="width: 150px;" class="col-md-2">
        RDD to :
        <input style="width:120px;" class="cls_my_datepicker form-control cls_so_new_rdd_to" />
    </div>
    <div style="width: 150px;" class="col-md-2">
        Packaging type :
        @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_new_packaging_type" } })
    </div>
    <div style="width: 150px;" class="col-md-2">
        Sold to :
        @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_new_sold_to" } })
    </div>
    <div style="width: 150px;" class="col-md-2">
        Ship to :
        @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_new_ship_to" } })
    </div>
    <div style="width: 150px;" class="col-md-2">
        Brand :
        @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_new_brand" } })
    </div>
</div>
<div class="row">
    <div style="width: 150px;" class="col-md-2">
        Grouping :
        <select class="cls_lov_so_new_group" style="width:100%;">
            <option value="yes">Yes</option>
            <option value="no">No</option>
        </select>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <button type="button" class="btn btn-primary cls_btn_search_so_new">Search</button>
        <button type="button" class="btn btn-info cls_btn_excel_so_new">Export excel</button>
    </div>
</div>
<table id="table_so_new" class="display" style="width:100%">
    <thead>
        <tr>
            <th class="cls_nowrap">No.</th>
            <th class="cls_nowrap"></th>
            <th class="cls_nowrap"></th>
            <th class="cls_nowrap">SO number</th>
            <th class="cls_nowrap">SO item</th>
            <th class="cls_nowrap">Product code</th>
            <th class="cls_nowrap">Sales org</th>
            <th class="cls_nowrap">Production Plant</th>
            <th class="cls_nowrap">Sold to name</th>
            <th class="cls_nowrap">Ship to name</th>
            <th class="cls_nowrap">Brand</th>
            <th class="cls_nowrap">Additional brand</th>
            <th class="cls_nowrap">Country</th>
            <th class="cls_nowrap">In transit to</th>
            <th class="cls_nowrap">Item</th>
            <th class="cls_nowrap">Order Bom</th>
            <th class="cls_nowrap">Component description</th>
            <th class="cls_nowrap">New Bom item</th>
            <th class="cls_nowrap">Create on order</th>
            <th class="cls_nowrap">RDD</th>

        </tr>
    </thead>
</table>
