@{
    ViewBag.Title = "Warehouse report";
}
<div class="cls_report_warehouse">
    <div class="panel-default">
        <div class="panel-heading cls_hand cls_div_search_criteria">
            <h3 class="panel-title">Warehouse report</h3>
        </div>
        <div class="panel-body cls_div_body_search_criteria" style="padding:0px;padding-top:10px;">
            <form>
                <div class="row">
                    <div class="col-md-2 control-label">
                        Sales order <span style="color:red;">*</span>:
                    </div>
                    <div class="col-md-4">
                        <input type="text" class="form-control cls_txt_sales_order" />
                    </div>
                    <div class="col-md-2 control-label">
                        Sales organization <span style="color:red;">*</span>:
                    </div>
                    <div class="col-md-4">
                        @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_sales_organization" } })
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-2 control-label">
                        Sales order item :
                    </div>
                    <div class="col-md-4">
                        <input type="text" class="form-control cls_txt_sales_order_item" />
                    </div>
                    <div class="col-md-2 control-label">
                        Ship-to :
                    </div>
                    <div class="col-md-4">
                        @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_ship_to" } })
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-2 control-label">
                        Brand :
                    </div>
                    <div class="col-md-4">
                        @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_brand" } })
                    </div>
                    <div class="col-md-2 control-label">
                        Project name :
                    </div>
                    <div class="col-md-4">
                        <input type="text" class="form-control cls_txt_project_name" />
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-md-2 control-label">
                        Purchase order <span style="color:red;">*</span>:
                    </div>
                    <div class="col-md-4">
                        <input type="text" class="form-control cls_txt_purchase_order" />
                    </div>
                    <div class="col-md-2 control-label">
                        Purchasing organization :
                    </div>
                    <div class="col-md-4">
                        @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_purchasing_organization" } })
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-2 control-label">
                        Purchase order item :
                    </div>
                    <div class="col-md-4">
                        <input type="text" class="form-control cls_txt_purchase_order_item" />
                    </div>
                    <div class="col-md-2 control-label">
                        Packaging material :
                    </div>
                    <div class="col-md-4">
                        @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_packaging_material" } })
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-2 control-label">
                        Company :
                    </div>
                    <div class="col-md-4">
                        @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_company" } })
                    </div>
                    <div class="col-md-2 control-label">
                        Document date :
                    </div>
                    <div class="col-md-4">
                        <input type="text" class="cls_dt_document_date cls_my_datepicker form-control" placeholder="dd/mm/yyyy" />
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-4">
                        <button type="submit" class="cls_btn_search btn btn-primary">Search</button>
                        <button type="button" class="cls_btn_clear btn btn-danger">Clear</button>
                        <button type="button" class="btn btn-info cls_btn_export_excel">Export excel</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
    <hr style="margin-top:10px;margin-bottom:10px;" />
    <div class="row">
        <div class="col-md-12 cls_table_report_warehouse">
            <table id="table_report_warehouse" class="display" style="width:100%">
                <thead>
                    <tr>
                        <th class="cls_nowrap">No. </th>
                        <th class="cls_nowrap">Sales org.</th>
                        <th class="cls_nowrap">SO no.</th>
                        <th class="cls_nowrap">Item</th>
                        <th class="cls_nowrap">SO Att.</th>
                        <th class="cls_nowrap">AW with SO &nbsp;&nbsp;&nbsp;Final AW</th>
                        <th class="cls_nowrap">Product code</th>
                        <th class="cls_nowrap">Material code</th>
                        <th class="cls_nowrap">Material description</th>
                        <th class="cls_nowrap">Pkg. Type</th>
                        <th class="cls_nowrap">AW WF no.</th>
                        <th class="cls_nowrap">Brand</th>
                        <th class="cls_nowrap">Project name</th>
                        <th class="cls_nowrap">Sold-to</th>
                        <th class="cls_nowrap">Sold-to name</th>
                        <th class="cls_nowrap">Ship-to</th>
                        <th class="cls_nowrap">Ship-to name</th>
                        <th class="cls_nowrap">Port</th>
                        <th class="cls_nowrap">PO no.</th>
                        <th class="cls_nowrap">Item</th>
                        <th class="cls_nowrap">PO Att.</th>
                        <th class="cls_nowrap">Purch. Org.</th>
                        <th class="cls_nowrap">Vendor</th>
                        <th class="cls_nowrap">Vendor name</th>
                        <th class="cls_nowrap">Doc. date</th>
                        <th class="cls_nowrap">Deliv. date</th>
                        <th class="cls_nowrap">Quantity</th>
                        <th class="cls_nowrap">Unit</th>
                    </tr>
                </thead>
            </table>
        </div>
    </div>
</div>

<!-- Modal SO Att -->
<div class="modal" id="modal_report_warehouse_so_att" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="vertical-alignment-helper">
        <input type="hidden" class="cls_so_att_salesorderno" />
        <div class="modal-dialog  modal_in_modal vertical-align-center" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title" id="myModalLabel">SO Attachment</h4>
                </div>
                <div class="modal-body">
                    <div class="cls_div_table_report_warehouse_so_att">
                        <table id="table_report_warehouse_so_att" class="display" style="width:100%">
                            <thead>
                                <tr>
                                    <th class="cls_nowrap"></th>
                                    <th style="width:100px;">Title</th>
                                    <th class="cls_nowrap">File name</th>
                                    <th style="width:150px;">Created by</th>
                                    <th style="width:150px;">Created date</th>
                                    <th style="width:60px;">Action</th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Modal PO Att -->
<div class="modal" id="modal_report_warehouse_po_att" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="vertical-alignment-helper">
        <input type="hidden" class="cls_po_att_pono" />
        <div class="modal-dialog  modal_in_modal vertical-align-center" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title" id="myModalLabel">PO Attachment</h4>
                </div>
                <div class="modal-body">
                    <div class="cls_div_table_report_warehouse_po_att">
                        <table id="table_report_warehouse_po_att" class="display" style="width:100%">
                            <thead>
                                <tr>
                                    <th class="cls_nowrap"></th>
                                    <th style="width:100px;">Title</th>
                                    <th class="cls_nowrap">File name</th>
                                    <th style="width:150px;">Created by</th>
                                    <th style="width:150px;">Created date</th>
                                    <th style="width:60px;">Action</th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Modal AW Att -->
<div class="modal" id="modal_report_warehouse_aw_att" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="vertical-alignment-helper">
        <input type="hidden" class="cls_aw_att_mat_no" />
        <input type="hidden" class="cls_aw_att_mat_desc" />
        <div class="modal-dialog  modal_in_modal vertical-align-center" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title" id="myModalLabel">AW Attachment</h4>
                </div>
                <div class="modal-body">
                    <div class="cls_div_table_report_warehouse_aw_att">
                        <table id="table_report_warehouse_aw_att" class="display" style="width:100%">
                            <thead>
                                <tr>
                                    <th class="cls_nowrap"></th>
                                    <th style="width:100px;">Title</th>
                                    <th class="cls_nowrap">File name</th>
                                    <th style="width:150px;">Created by</th>
                                    <th style="width:150px;">Created date</th>
                                    <th style="width:60px;">Action</th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
</div>
@section scripts
{
    @Scripts.Render("~/bundles/reportwarehouse")
}
