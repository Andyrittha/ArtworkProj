                                <div class="col-md-3 control-label">
                                    ผลทดสอบความยาก/ง่าย :
                                </div>
                                <div class="col-md-4">
                                    <label class="radio-inline"><input type="radio" name="wh_difficult" value="1">ยาก</label>
                                    <label class="radio-inline"><input type="radio" name="wh_difficult" value="0">ง่าย</label>
                                </div>
                            </div>
                            <div class="row">

                                <div class="col-md-offset-3 col-md-9">
                                    <label class="error cls_error" for="wh_difficult"></label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3">
                                    Comment :
                                </div>
                                <div class="col-md-9">
                                    <textarea class="form-control cls_whmu_txt_comment" rows="5"></textarea>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3 control-label">
                                    Need to commissioning packaging :
                                </div>
                                <div class="col-md-4">
                                    <label class="radio-inline"><input type="radio" name="wh_need_to_com" value="1">Yes</label>
                                    <label class="radio-inline"><input type="radio" name="wh_need_to_com" value="0">No</label>
                                </div>
                            </div>
                            <div class="row">

                                <div class="col-md-offset-3 col-md-9">
                                    <label class="error cls_error" for="wh_need_to_com"></label>
                                </div>
                            </div>
                        </div>
                        @Html.Partial("ColorInfo")
                        <div class="row">
                            <div class="col-md-12">
                                <table id="table_wh_log" class="display" style="width:100%">
                                    <thead>
                                        <tr>
                                            <th class="cls_nowrap cls_th_log_by_pg">No.</th>
                                            <th class="cls_nowrap cls_th_log_by_pg">เพื่อยืนยันขนาด</th>
                                            <th class="cls_nowrap cls_th_log_by_pg">เพื่อตรวจสอบ<br />ความยาก/ง่าย ในการบรรจุ</th>
                                            <th class="cls_nowrap cls_th_log_by_pg">Supplier<br />primary container</th>
                                            <th class="cls_nowrap cls_th_log_by_pg">Supplier<br />primary lid</th>
                                            <th class="cls_nowrap cls_th_log_by_pg">สิ่งที่จัดส่งไปโรงงาน</th>
                                            <th class="cls_nowrap cls_th_log_by_pg">Reason by PG</th>
                                            <th class="cls_nowrap cls_th_log_by_pg">Remark reason by PG</th>
                                            <th class="cls_nowrap cls_th_log_by_pg">Comment by PG</th>
                                            <th class="cls_nowrap cls_th_log_by_pg">Created date-time</th>
                                            <th class="cls_nowrap">Action name</th>
                                            <th class="cls_nowrap">Reason</th>
                                            <th class="cls_nowrap">Remark reason</th>
                                            <th class="cls_nowrap">Receive physical<br />for test pack</th>
                                            <th class="cls_nowrap">Test pack result</th>
                                            <th class="cls_nowrap">Need to<br />commissioning<br />packaging</th>
                                            <th class="cls_nowrap">ผลทดสอบ<br />ความยาก/ง่าย</th>
                                            <th class="cls_nowrap">Comment</th>
                                            <th class="cls_nowrap">Create date-time</th>
                                            @*<th class="cls_nowrap">Create by</th>*@
                                        </tr>
                                    </thead>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </form>
    </div>

