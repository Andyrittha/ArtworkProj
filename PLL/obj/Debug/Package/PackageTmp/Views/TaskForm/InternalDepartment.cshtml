<div class="cls_task_form_intdep" style="padding-top:10px;">
    <div class="row cls_rdmu_row cls_hide">
        <div class="col-md-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title">R&D</h3>
                </div>
                <div class="panel-body">
                    <div class="cls_div_only_rd cls_hide">
                        <div class="row">
                            <div class="col-md-12">
                                <button type="button" class="cls_rdmu_btn_send_primary btn btn-success">Send primary packaging</button>
                                <button type="button" class="cls_rdmu_btn_send_back btn btn-warning">Send back workflow</button>
                                <button type="button" class="cls_rdmu_btn_save btn btn-info">Save & send information</button>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <button type="button" data-toggle="modal" data-target="#popup_attachment" data-go_to_step="SEND_RD_PRI_PKG" class="btn btn-default">View/upload attachment</button>
                        </div>
                    </div>
                    <div class="cls_div_only_rd cls_hide">
                        <div class="row">
                            <div class="col-md-2 control-label">
                                Reason for revise/cancel/overdue :
                            </div>
                            <div class="col-md-5">
                                @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_send_for_reason" }, { "cls_txtarea", "cls_input_other" } })
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-2 control-label">
                                Final sample :
                            </div>
                            <div class="col-md-10">
                                <label class="radio-inline"><input type="radio" name="rd_final_sample" value="1">Yes</label>
                                <label class="radio-inline"><input type="radio" name="rd_final_sample" value="0">No</label>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-2 control-label">
                                Type of sample :
                            </div>
                            <div class="col-md-10">
                                <label class="radio-inline"><input type="radio" name="rd_type_of_sample" value="RD">RD sample</label>
                                <label class="radio-inline"><input type="radio" name="rd_type_of_sample" value="PRD">Production sample</label>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-2">
                                Comment :
                            </div>
                            <div class="col-md-10">
                                <textarea class="form-control cls_rdmu_txt_comment" rows="5"></textarea>
                            </div>
                        </div>
                    </div>
                    @Html.Partial("ColorInfo")
                    <div class="row">
                        <div class="col-md-12">
                            <table id="table_rd_log" class="display" style="width:100%">
                                <thead>
                                    <tr>
                                        <th class="cls_nowrap cls_th_log_by_pg">No.</th>
                                        <th class="cls_nowrap cls_th_log_by_pg">Reason by PG</th>
                                        <th class="cls_nowrap cls_th_log_by_pg">Remark reason by PG</th>
                                        <th class="cls_nowrap cls_th_log_by_pg">Comment by PG</th>
                                        <th class="cls_nowrap cls_th_log_by_pg">Created date-time</th>
                                        <th class="cls_nowrap">Action name</th>
                                        <th class="cls_nowrap">Reason</th>
                                        <th class="cls_nowrap">Remark reason</th>
                                        <th class="cls_nowrap">Final sample</th>
                                        <th class="cls_nowrap">Type of sample</th>
                                        <th class="cls_nowrap">Comment by RD</th>
                                        <th class="cls_nowrap">Created date-time</th>
                                        @*<th class="cls_nowrap">Create by</th>*@
                                    </tr>
                                </thead>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

