                        <div class="cls_div_only_wh cls_hide">
                            <div class="row">
                                <div class="col-md-3 control-label">
                                    Reason for revise/cancel/overdue :
                                </div>
                                <div class="col-md-5">
                                    @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_send_for_reason" }, { "cls_txtarea", "cls_input_other" }  })
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3 control-label">
                                    Receive Physical for test pack :
                                </div>
                                <div class="col-md-9">
                                    <label class="checkbox-inline"><input type="checkbox" class="cls_whmu_chk_received" value="">Received</label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3 control-label">
                                    Test pack result :
                                </div>
                                <div class="col-md-9">
                                    <label class="radio-inline"><input type="radio" name="whmu_rdo_test_pack_result" value="1">Pass</label>
                                </div>
                            </div>
