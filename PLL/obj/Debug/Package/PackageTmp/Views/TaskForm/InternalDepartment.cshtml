                                <div class="col-md-3">
                                </div>
                                <div class="col-md-1">
                                    <label class="radio-inline"><input type="radio" name="whmu_rdo_test_pack_result" value="0">Fail</label>
                                </div>
                                <div class="col-md-3">
                                    @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_whmu_lov_test_pack_result_reject" } })
                                </div>
                            </div>
                            <div class="row">

                                <div class="col-md-offset-3 col-md-9">
                                    <label class="error cls_error" for="whmu_rdo_test_pack_result"></label>
                                </div>
                            </div>
