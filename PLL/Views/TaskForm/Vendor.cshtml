                                ID(mm)   :
                            </div>
                            <div class="col-md-4">
                                <input disabled type="text" class="cls_vn_dimension_of form-control" />
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-2 control-label">
                                Specification   :
                            </div>
                            <div class="col-md-4">
                                <input disabled type="text" class="cls_vn_spec form-control" />
                            </div>
                            <div class="col-md-2 control-label">
                                Flute   :
                            </div>
                            <div class="col-md-4">
                                <input disabled type="text" class="cls_vn_flute form-control" />
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-2 control-label">
                                Accessories :
                            </div>
                            <div class="col-md-4">
                                <input disabled type="text" class="cls_vn_acc form-control" />
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-2 control-label">
                                จำนวนตัวอย่างที่ขอ  :
                            </div>
                            <div class="col-md-4">
                                <input disabled type="text" class="cls_vn_amount form-control" />
                            </div>
                        </div>
                    </div>

                    <div class="cls_div_only_vendor cls_hide">
                        <br>
                        <div class="row">
                            <div class="col-md-2">
                                Topic :
                            </div>
                            <div class="col-md-7" style="font-weight: bold">
                                Send primary and request sample <br>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-2 control-label">
                                Send for (Reason) :
                            </div>
                            <div class="col-md-5">
                                @*//---------------------------------changed by aof #INC-11265------------------------------------------------------------------------------*@
                                @*  @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_send_for_reason" } })*@
                                @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_send_for_reason" }, { "cls_txtarea", "cls_input_other" } })
                                @*//---------------------------------changed by aof #INC-11265------------------------------------------------------------------------------*@

                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-2">
                                Comment :
                            </div>
                            <div class="col-md-7">
                                <textarea class="form-control cls_vendor_txt_comment" rows="5"></textarea>
                            </div>
                        </div>
                        <br>
                    </div>
                    @Html.Partial("ColorInfo")
                    <div class="row">
                        <div class="col-md-12">
                            <table id="table_vendor_log_pr" class="display" style="width:100%">
                                <thead>
                                    <tr>
                                        <th class="cls_nowrap cls_th_log_by_pg">No.</th>
                                        <th class="cls_nowrap cls_th_log_by_pg">Vendor name</th>
                                        <th class="cls_nowrap cls_th_log_by_pg">Reason by PG</th>
                                        <th class="cls_nowrap cls_th_log_by_pg">Remark reason by PG</th>
                                        <th class="cls_nowrap cls_th_log_by_pg">Comment by PG</th>
                                        <th class="cls_nowrap cls_th_log_by_pg">Created date-time</th>
                                        <th class="cls_nowrap">Action name</th>
                                        <th class="cls_nowrap">Reason</th>
                                        <th class="cls_nowrap">Remark reason</th>
                                        <th class="cls_nowrap">Comment by vendor</th>
