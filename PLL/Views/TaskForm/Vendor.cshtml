                    <div class="cls_div_only_vendor cls_hide">
                        <div class="row">
                            <div class="col-md-12">
                                <button type="button" class="cls_btn_submit_vendor btn btn-primary">Submit</button>
                                <button type="button" class="cls_btn_send_back_vendor btn btn-warning">Send back workflow</button>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <button type="button" data-toggle="modal" data-target="#popup_attachment" data-go_to_step="SEND_VN_PR" class="btn btn-default">View/upload attachment</button>
                        </div>
                    </div>
                    <div class="cls_div_only_vendor cls_hide">
                        <div class="row">
                            <div class="col-md-2 control-label" style="font-weight: bold">
                                Packaging type  :
                            </div>
                            <div class="col-md-4"><input disabled type="text" class="form-control cls_vn_packaging_type" /></div>

                            <div class="col-md-2 control-label" style="font-weight: bold">
                                Brand   :
                            </div>
                            <div class="col-md-4"><input disabled type="text" class="form-control cls_vn_brand" /></div>
                        </div>

                        <div class="row">
                            <div class="col-md-2 control-label">
                                Primary type    :
                            </div>
                            <div class="col-md-4">
                                <input disabled type="text" class="cls_vn_primary_type form-control" />
