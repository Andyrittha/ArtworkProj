<div class="modal" id="pg_submit_modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel">Sent to internal</h4>
            </div>
            <form>
                <div class="modal-body">
                    <div class="row" style="padding-bottom:10px;">
                        <div class="col-md-11">
                            @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_search_file_template" } })
                        </div>
                        <div class="col-md-1 control-label">
                            <a href="@Url.Action("DownloadFileTemplate","FileUpload")" style="" class="cls_link_load_file_template cls_hand" target="_blank">Download</a>
                        </div>
                    </div>
                    <fieldset class="scheduler-border">
                        <legend class="scheduler-border2">Request for primary packaging</legend>
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <label class="checkbox-inline"><input type="checkbox" class="cls_header_chk cls_chk_send_rd">Send to R&D</label>
                            </div>
                            <div class="panel-body cls_body_send_rd" style="display:none;">
                                <div class="row">
                                    <div class="col-md-3 control-label">
                                        Send for (Reason) :
                                    </div>
                                    <div class="col-md-9">
                                        @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_send_for_reason" }, { "cls_txtarea", "cls_input_send_rd_other" } })
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="cls_txtedt_send_rd" style="height: 80px;"></div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-7">

                                    </div>
                                    <div style="text-align: right;" class="col-md-5">
                                        <a class="cls_hand" data-go_to_step="SEND_RD_PRI_PKG" data-toggle="modal" data-target="#popup_attachment">Upload Attachment</a>&nbsp;&nbsp;&nbsp;
                                        <a class="cls_hand" data-toggle="modal" data-target="#pg_master_template">Copy Template</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <label class="checkbox-inline"><input type="checkbox" class="cls_header_chk cls_chk_send_planning">Send to planning</label>
                            </div>
                            <div class="panel-body cls_body_send_planning" style="display:none;">
                                <div class="row">
                                    <div class="col-md-3 control-label">
                                        Send for (Reason) :
                                    </div>
                                    <div class="col-md-9">
                                        @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_send_for_reason" }, { "cls_txtarea", "cls_input_send_planning_other" } })
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="cls_txtedt_send_planning" style="height: 80px;"></div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-7">

                                    </div>
                                    <div style="text-align: right;" class="col-md-5">
                                        <a class="cls_hand" data-go_to_step="SEND_PN_PRI_PKG" data-toggle="modal" data-target="#popup_attachment">Upload Attachment</a>&nbsp;&nbsp;&nbsp;
                                        <a class="cls_hand" data-toggle="modal" data-target="#pg_master_template">Copy Template</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </fieldset>



                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <label class="checkbox-inline"><input type="checkbox" class="cls_header_chk cls_chk_send_supervisor"><span class="cls_lbl_submit_to_supervisor">Send to supervisor</span></label>
                        </div>
                        <div class="panel-body cls_body_send_supervisor" style="display:none;">
                            <div class="row">
                                <div class="col-md-3 control-label">
                                    Send for (Reason) :
                                </div>
                                <div class="col-md-9">
                                    @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_send_for_reason" } , { "cls_txtarea", "cls_input_send_supervisor_other" }})
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="cls_txtedt_send_supervisor" style="height: 80px;"></div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-7">

                                </div>
                                <div style="text-align: right;" class="col-md-5">
                                    <a class="cls_hand" data-go_to_step="SEND_PG_SUP_SEL_VENDOR" data-toggle="modal" data-target="#popup_attachment">Upload Attachment</a>&nbsp;&nbsp;&nbsp;
                                    <a class="cls_hand" data-toggle="modal" data-target="#pg_master_template">Copy Template</a>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <label class="checkbox-inline"><input type="checkbox" class="cls_header_chk cls_chk_send_supervisor_need_design">Send to supervisor (need design)</label>
                        </div>
                        <div class="panel-body cls_body_send_supervisor_need_design" style="display:none;">
                            <div class="row">
                                <div class="col-md-3 control-label">
                                    Send for (Reason) :
                                </div>
                                <div class="col-md-9">
                                    @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_send_for_reason" }, { "cls_txtarea", "cls_input_send_supervisor_need_design_other" } })
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="cls_txtedt_send_supervisor_need_design" style="height: 80px;"></div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-7">

                                </div>
                                <div style="text-align: right;" class="col-md-5">
                                    <a class="cls_hand" data-go_to_step="SEND_PG_SUP_SEL_VENDOR_NEED_DESIGN" data-toggle="modal" data-target="#popup_attachment">Upload Attachment</a>&nbsp;&nbsp;&nbsp;
                                    <a class="cls_hand" data-toggle="modal" data-target="#pg_master_template">Copy Template</a>
                                </div>
                            </div>
                        </div>
                    </div>


                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <label class="checkbox-inline"><input type="checkbox" class="cls_header_chk cls_chk_send_warehouse">Send to warehouse for test pack</label>
                        </div>
                        <div class="panel-body cls_body_send_warehouse" style="display:none;">

                            <div class="row">
                                <div class="col-md-2">
                                    <label class="checkbox-inline"><input type="checkbox" name="send_to_warehouse" class="cls_chk_confirm_size">เพื่อยืนยันขนาด</label>
                                </div>
                                <div class="col-md-6">
                                    <label class="checkbox-inline"><input type="checkbox" name="send_to_warehouse" class="cls_chk_easy">เพื่อตรวจสอบความยาก/ง่าย ในการบรรจุ</label>
                                </div>
                            </div>
                            <div class="row">

                                <div class="col-md-12">
                                    <label class="error cls_error" for="send_to_warehouse"></label>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-2 control-label">
                                    Customer :
                                </div>
                                <div class="col-md-4">
                                    <input disabled type="text" class="form-control cls_wh_customer" />
                                </div>
                                <div class="col-md-2 control-label">
                                    Brand :
                                </div>
                                <div class="col-md-4">
                                    <input disabled type="text" class="form-control cls_wh_brand" />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-2 control-label">
                                    Primary type :
                                </div>
                                <div class="col-md-4">
                                    <input disabled type="text" class="form-control cls_wh_primary_type" />
                                </div>
                                <div class="col-md-2 control-label cls_nowrap">
                                    Supplier primary container<span style="color:red;">*</span> :
                                </div>
                                <div class="col-md-4">
                                    <input type="text" class="form-control cls_wh_supplier_primary_container" />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-2 control-label">
                                    Primary size :
                                </div>
                                <div class="col-md-4">
                                    <input disabled type="text" class="form-control cls_wh_primary_size" />
                                </div>
                                <div class="col-md-2 control-label">
                                    Supplier primary lid<span style="color:red;">*</span> :
                                </div>
                                <div class="col-md-4">
                                    <input type="text" class="form-control cls_wh_supplier_primary_lid" />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-2 control-label">
                                    Pack size :
                                </div>
                                <div class="col-md-4">
                                    <input disabled type="text" class="form-control cls_wh_pack_size" />
                                </div>
                                <div class="col-md-2 control-label">
                                    Packaging type :
                                </div>
                                <div class="col-md-4">
                                    <input disabled type="text" class="form-control cls_wh_packaging_type" />
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-2 control-label">
                                    Packing style :
                                </div>
                                <div class="col-md-4">
                                    <input disabled type="text" class="form-control cls_wh_packaging_style" />
                                </div>
                                <div class="col-md-2 control-label">
                                    Grade of :
                                </div>
                                <div class="col-md-4">
                                    <input disabled type="text" class="form-control cls_wh_grade" />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-2 control-label">
                                    Net weight :
                                </div>
                                <div class="col-md-4">
                                    <input disabled type="text" class="form-control cls_wh_net_weight" />
                                </div>
                                <div class="col-md-2 control-label cls_nowrap">
                                    Dimension of (LXWXH)(mm.) :
                                </div>
                                <div class="col-md-4">
                                    <input disabled type="text" class="form-control cls_wh_dimension_of" />
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-2 control-label">
                                    สิ่งที่จัดส่งไปโรงงาน<span style="color:red;">*</span> :
                                </div>
                                <div class="col-md-10">
                                    <input type="text" class="form-control cls_wh_remark" />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-2 control-label">
                                    Send for (Reason) :
                                </div>
                                <div class="col-md-10">
                                    @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_send_for_reason" }, { "cls_txtarea", "cls_input_send_warehouse_other" } })
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="cls_txtedt_send_warehouse" style="height: 80px;"></div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-7">

                                </div>
                                <div style="text-align: right;" class="col-md-5">
                                    <a class="cls_hand" data-go_to_step="SEND_WH_TEST_PACK" data-toggle="modal" data-target="#popup_attachment">Upload Attachment</a>&nbsp;&nbsp;&nbsp;
                                    <a class="cls_hand" data-toggle="modal" data-target="#pg_master_template">Copy Template</a>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <label class="checkbox-inline"><input type="checkbox" class="cls_header_chk cls_chk_send_approval_match_board">Send for approval match board</label>
                        </div>
                        <div class="panel-body cls_body_send_approval_match_board" style="display:none;">
                            <div class="row">
                                <div class="col-md-3 control-label">
                                    Send for (Reason) :
                                </div>
                                <div class="col-md-9">
                                    @Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_send_for_reason" }, { "cls_txtarea", "cls_input_send_approval_match_board_other" } })
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="cls_txtedt_send_approval_match_board" style="height: 80px;"></div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-7">

                                </div>
                                <div style="text-align: right;" class="col-md-5">
                                    <a class="cls_hand" data-go_to_step="SEND_APP_MATCH_BOARD" data-toggle="modal" data-target="#popup_attachment">Upload Attachment</a>&nbsp;&nbsp;&nbsp;
                                    <a class="cls_hand" data-toggle="modal" data-target="#pg_master_template">Copy Template</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="submit" class="cls_btn_submit btn btn-primary">Submit</button>
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </div>
            </form>
        </div>
    </div>
</div>
