
@{
    ViewBag.Title = "IGrid Summary Report";
}
<div class="cls_report_igridsummary">
    <div class="panel-default">
        <div class="panel-heading cls_div_search_criteria">
            <h3 class="panel-title">IGrid Summary Report</h3>
        </div>

        <div class="panel-body cls_div_body_search_criteria" style="padding:0px;padding-top:10px;">
            <form>
                <div class="row">
                    <div class="col-md-2 control-label">
                        By :
                    </div>
                    <div class="col-md-4">
                        <select class="form-control cls_lov_action" style="width:100%;">
                            <option value="All" selected>All</option>
                            <option value="Complete">Complete</option>
                            <option value="Info_group">Info group</option>
                        </select>
                    </div>
                    <div class="col-md-2 control-label">Keyword :<font color="red">*</font></div>
                    <div class="col-md-4">
                        <input type="text" class="form-control cls_txt_master_keyword" placeholder="" />
                        <label style="font-weight: normal;font-style:italic;color:gray;" class="cls_lbl_mater_keywork_description">Material No, Material Description, Document No, Brand</label>
                    </div>
                </div>
                <hr />
                <div class="row">
                    <div class="col-md-4">
                        <button type="button" class="cls_igrids_btn_search btn btn-primary">Search</button>
                        <button type="button" class="cls_igrids_btn_clr btn btn-danger">Clear</button>
                        <button type="button" class="btn btn-info cls_btn_export_excel">Export excel</button>
                        @*<button type="button" class="cls_igrids_btn_Remove btn btn-danger">Inactive</button>
                            <button type="button" class="cls_trk_btn_back btn btn-warning">Back</button>*@
                    </div>
                </div>
            </form>
        </div>
    </div>
    <hr style="margin-top:10px;margin-bottom:10px;" />
    <div class="row">
        <div class="col-md-12">
            <!--Packaging View-->
            <div class="cls_table_report_igrids">
                <table id="table_report_igrids" class="display" style="width:100%">
                    <thead>
                        <tr>
                            @*<th class="cls_nowrap"></th>*@
                            <th class="cls_nowrap">Id</th>
                            <th class="cls_nowrap">Condition</th>
                            <th class="cls_nowrap">RequestType</th>
                            <th class="cls_nowrap">DocumentNo</th>
                            <th class="cls_nowrap">DMS No./Artwork</th>
                            <th class="cls_nowrap">Material</th>
                            <th class="cls_nowrap">Description</th>
                            <th class="cls_nowrap">Group</th>
                            <th class="cls_nowrap">Brand</th>
                            <th class="cls_nowrap">CreateOn</th>
                            <th class="cls_nowrap">PA UserName</th>
                            <th class="cls_nowrap">PG UserName</th>
                            <th class="cls_nowrap">FinalInfoGroup</th>
                            @*<th class="cls_nowrap">Remark</th>*@

                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <!--Packaging View-->
            <div class="cls_table_report_igrids_all">
                <table id="table_report_igrids_all" class="display" style="width:100%">
                    <thead>
                        <tr>
                            <th class="cls_nowrap"></th>
                            <th class="cls_nowrap">Id</th>
                            <th class="cls_nowrap">Condition</th>
                            <th class="cls_nowrap">RequestType</th>
                            <th class="cls_nowrap">DocumentNo</th>
                            <th class="cls_nowrap">DMS No./Artwork</th>
                            <th class="cls_nowrap">Material No.</th>
                            <th class="cls_nowrap">Description</th>
                            <th class="cls_nowrap">Group</th>
                            <th class="cls_nowrap">Brand</th>

                            <th class="cls_nowrap">Role</th>
                            <th class="cls_nowrap">CreateOn</th>
                            <th class="cls_nowrap">ActiveBy</th>
                            <th class="cls_nowrap">StatusApp</th>
                            @*<th class="cls_nowrap">Remark</th>*@
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <!--Packaging View-->
            <div class="cls_table_report_igrids_infogroup">
                <table id="table_report_igrids_infogroup" class="display" style="width:100%">
                    <thead>
                        <tr>
                          
                            @*<th class="cls_nowrap"></th>*@
                            <th class="cls_nowrap">Id</th>
                            <th class="cls_nowrap">Condition</th>
                            <th class="cls_nowrap">RequestType</th>
                            <th class="cls_nowrap">DocumentNo</th>
                            <th class="cls_nowrap">DMS No./Artwork</th>
                            <th class="cls_nowrap">Material No.</th>
                            <th class="cls_nowrap">Description</th>
                            <th class="cls_nowrap">Group</th>
                            <th class="cls_nowrap">Brand</th>
                            <th class="cls_nowrap">Assignee(PG Name)</th>
                            <th class="cls_nowrap">CreateOn</th>
                            <th class="cls_nowrap">ActiveBy(PA Name)</th>
                            <th class="cls_nowrap">FinalInfoGroup</th>
                            @*<th class="cls_nowrap">ReferenceMaterial</th>
                            <th class="cls_nowrap">Vendor</th>
                            <th class="cls_nowrap">Vendor Description</th>*@
                         
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>
@section scripts
{
    @Scripts.Render("~/bundles/reportigridsummary")
}


