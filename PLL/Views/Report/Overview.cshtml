
@{
    ViewBag.Title = "Overview";
}
<div class="cls_report_overview">
    <div class="panel-default">
        <div class="panel-heading cls_div_search_criteria">
            <h3 class="panel-title">Overview report</h3>
        </div>

        <div class="panel-body cls_div_body_search_criteria" style="padding:0px;padding-top:10px;">
            <form>
                <div class="row">
                    <div class="col-md-2 control-label">
                        By :
                    </div>
                    <div class="col-md-4">
                        <select class="form-control cls_lov_condition" style="width:100%;">
                            <option value="All" selected>All</option>
                            @*<option value="Known SO">Known SO</option>
                    <option value="Unknown SO">Unknown SO</option>
                    <option value="Project">Project</option>
                    <option value="Version">Version</option>
                    <option value="Change Classification">Change Classification</option>*@
                        </select>
                        @*@Html.Partial("LOV/ListOfValue", new ViewDataDictionary { { "cls_ddl", "cls_lov_condition" } })*@
                    </div>
                    <div class="col-md-2 control-label">Keyword :<font color="red">*</font></div>
                    <div class="col-md-4"><input type="text" class="form-control cls_txt_master_keyword" placeholder="" /></div>

                </div>
                <div class="row">
                    <div class="col-md-2 control-label"></div>
                    <div class="col-md-4"></div>
                    <div class="col-md-2 control-label"></div>
                    <div class="col-md-4">
                        <label style="font-weight: normal;font-style:italic;color:gray;" class="cls_lbl_mater_keywork_description">Document No, Material No, AW WF No/DMS No</label>
                    </div>

                </div>
                <hr />
                <div class="row">
                    <div class="col-md-4">
                        <button type="submit" class="cls_over_btn_search btn btn-primary">Search</button>
                        <button type="button" class="cls_over_btn_clr btn btn-danger">Clear</button>
                        <button type="button" class="btn btn-info cls_btn_export_excel">Export excel</button>
                        <button type="button" class="cls_over_btn_Remove btn btn-danger cls_hide">Inactive</button>

                        <button type="button" class="btn btn_over_option cls_hide" style="background-color:darkorange;border-color:coral;color:white">Job Type</button>
                        @*<button type="button" class="cls_trk_btn_back btn btn-warning">Back</button>*@
                    </div>
                </div>
            </form>
        </div>
    </div>
    <hr style="margin-top:10px;margin-bottom:10px;" />
    <div class="row">
        <div class="col-md-12">
            <!--Packaging View-->
            <div class="cls_table_report_overview">
                <table id="table_report_overview" class="display" style="width:100%">
                    <thead>
                        <tr>
